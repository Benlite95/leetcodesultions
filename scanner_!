msg.num = ""
var doubleQuote = ' " ';
var std_name=""
var test = msg.payload
console.log(test)
var r_msg = test.msg_response.message;
var l_msg = test.msg_response.long_message;
var l_msgArray = l_msg.split(".");
var item = l_msgArray[0];
var p_points = l_msgArray[1];

var student = test.student;
if(student !== null){
    var std_name = `${student.first_name} ${student.last_name}`.slice(0,20);
    var std_points = `Totle Reward Points ${isNaN(parseInt(student.total_reward_points)) ? "0" : parseInt(student.total_reward_points)}`;
    var student_id = `${student.id}`
    flow.set("student_id",student_id);
    



    msg.payload = doubleQuote + r_msg + doubleQuote + doubleQuote + std_name + doubleQuote + doubleQuote + item + doubleQuote + doubleQuote + p_points + doubleQuote + doubleQuote + std_points + doubleQuote
}
if( r_msg == "Ticket Was Redeemed." ){  
    msg.num = 10 
    }
return msg;
